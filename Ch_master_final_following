# Here's what is used for chartered children after you've observed changes

CREATE TABLE `Royal_rangers.fromyear_toyear_ch_l2` AS (
  SELECT
    ROW_NUMBER() OVER (ORDER BY First_att, Unch_name) AS Unch_num,
    Ch_name,
    Ch_date
  FROM `Royal_rangers.fromyear_toyear_ch_l1`
);

DROP TABLE `Royal_rangers.fromyear_toyear_chartered_list`;

CREATE TABLE `Royal_rangers.fromyear_toyear_chartered_list` AS (
  SELECT
    *
  FROM `Royal_rangers.fromyear_toyear_ch_l2`
);

DROP TABLE `Royal_rangers.fromyear_toyear_ch_l2`;
