# This is the list I want to create every week
# I want to print it off and write on it


WITH nw AS (
  SELECT
    Reg_n,
    Boy_s_name,
    Class_date
  FROM
    `your_table`
  WHERE
    Class_date > '2025-09-01'    -- Past 2 or 3 weeks
)

SELECT DISTINCT
  Reg_n,
  Boy_s_name,
  '' AS Attendance_5,
  '' AS On_time_5,
  '' AS Offering_10,
  '' AS Book_10,
  '' AS Bible_10,
  '' AS Uniform_5
FROM nw
ORDER BY Boy_s_name ASC;
