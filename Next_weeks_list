# This is the list I want to create every week
# I want to print it off and write on it


WITH nw AS (
  SELECT
    Reg_n,
    Boy_s_name,
    Class_date
  FROM
    `your_table`
  WHERE
    Class_date > '2025-09-01' -- Two or three past weeks
)

SELECT DISTINCT
  Reg_n,
  Boy_s_name
FROM nw
ORDER BY Boy_s_name ASC;
